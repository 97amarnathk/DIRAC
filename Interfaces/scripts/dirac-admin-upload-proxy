#!/usr/bin/env python
# $Header: /tmp/libdirac/tmp.stZoy15380/dirac/DIRAC3/DIRAC/Interfaces/scripts/Attic/dirac-admin-upload-proxy,v 1.2 2008/02/11 09:09:00 paterson Exp $
__RCSID__ = "$Id: dirac-admin-upload-proxy,v 1.2 2008/02/11 09:09:00 paterson Exp $"
import sys

def usage(argv):
  print 'Usage: %s <DIRAC proxy group>' %(argv[0])
  print 'The listing of DIRAC Roles is in the /Groups CS section'
  sys.exit(2)

if len(sys.argv)!=2:
  usage(sys.argv)

from DIRAC.Interfaces.API.DiracAdmin                         import DiracAdmin
diracAdmin = DiracAdmin()
try:
  group = str(sys.argv[1])
except Exception,x:
  print 'Expected string for DIRAC proxy group'
  sys.exit(2)

result = diracAdmin.uploadProxy(group)
if result['OK']:
  print 'Proxy uploaded for group %s' %(group)
  sys.exit(0)
else:
  print result['Message']
  sys.exit(2)